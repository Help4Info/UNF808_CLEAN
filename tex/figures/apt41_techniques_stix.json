    {
      "type": "indicator",
      "id": "indicator--rdp-apt41",
      "created": "2025-12-12T00:00:00.000Z",
      "modified": "2025-12-12T00:00:00.000Z",
      "name": "APT41 RDP Activity Pattern",
      "description": "Connexions RDP anormales depuis des comptes de service ou en dehors des heures de bureau",
      "pattern": "[network-traffic:dst_port = 3389 AND network-traffic:protocols[*] = 'tcp' AND user-account:account_type = 'service']",
      "pattern_type": "stix",
      "valid_from": "2021-01-01T00:00:00.000Z",
      "indicator_types": ["anomalous-activity"],
      "kill_chain_phases": [
        {
          "kill_chain_name": "mitre-attack",
          "phase_name": "lateral-movement"
        }
      ]
    },
    {
      "type": "indicator",
      "id": "indicator--smb-apt41",
      "created": "2025-12-12T00:00:00.000Z",
      "modified": "2025-12-12T00:00:00.000Z",
      "name": "APT41 SMB Lateral Movement",
      "description": "Accès aux partages administratifs Windows via SMB",
      "pattern": "[network-traffic:dst_port = 445 AND network-traffic:protocols[*] = 'smb' AND (file:name = 'C$' OR file:name = 'ADMIN$')]",
      "pattern_type": "stix",
      "valid_from": "2020-01-01T00:00:00.000Z",
      "indicator_types": ["malicious-activity"]
    },
    {
      "type": "indicator",
      "id": "indicator--wmi-apt41",
      "created": "2025-12-12T00:00:00.000Z",
      "modified": "2025-12-12T00:00:00.000Z",
      "name": "APT41 WMI Execution",
      "description": "Utilisation de WMI pour l'exécution distante via port 135",
      "pattern": "[process:name = 'wmiprvse.exe' AND network-traffic:dst_port = 135]",
      "pattern_type": "stix",
      "valid_from": "2020-01-01T00:00:00.000Z",
      "indicator_types": ["malicious-activity"]
    },
    {
      "type": "indicator",
      "id": "indicator--token-apt41",
      "created": "2025-12-12T00:00:00.000Z",
      "modified": "2025-12-12T00:00:00.000Z",
      "name": "APT41 Token Manipulation",
      "description": "Manipulation de tokens d'accès pour l'escalade de privilèges",
      "pattern": "[process:name = 'lsass.exe' AND process:opened_connection_refs[*].dst_port = 88]",
      "pattern_type": "stix",
      "valid_from": "2022-01-01T00:00:00.000Z",
      "indicator_types": ["anomalous-activity"]
    },
    {
      "type": "indicator",
      "id": "indicator--pth-apt41",
      "created": "2025-12-12T00:00:00.000Z",
      "modified": "2025-12-12T00:00:00.000Z",
      "name": "APT41 Pass-the-Hash Pattern",
      "description": "Pattern de Pass-the-Hash avec authentification NTLM sans mot de passe",
      "pattern": "[windows-registry-key:key = 'HKLM\\\\SAM\\\\SAM\\\\Domains\\\\Account\\\\Users' AND process:name = 'lsass.exe']",
      "pattern_type": "stix",
      "valid_from": "2020-01-01T00:00:00.000Z",
      "indicator_types": ["malicious-activity"]
    },
