    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c1-rdp-pth-combo",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Combined Attack: RDP Admin + Pass-the-Hash",
      "description": "Critical correlation detecting RDP admin access combined with Pass-the-Hash activity from same source IP. Rule: 110050",
      "pattern": "[network-traffic:dst_port = 3389 AND windows-event-log:event_id = 4624 AND windows-event-log:authentication_package = 'NTLM']",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c2-smb-wmi-combo",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Attack Chain: SMB Admin Share + WMI PowerShell",
      "description": "Critical correlation detecting SMB admin share access followed by WMI PowerShell execution. Rule: 110051",
      "pattern": "[file:path MATCHES '.*\\\\\\\\.*\\\\(C\\$|ADMIN\\$).*' AND process:parent_image_ref.name = 'WmiPrvSE.exe' AND process:image_ref.name = 'powershell.exe']",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c3-pth-wmi-combo",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Combined Attack: Pass-the-Hash + WMI Remote",
      "description": "Critical correlation confirming lateral movement via Pass-the-Hash and WMI execution. Rule: 110052",
      "pattern": "[windows-event-log:authentication_package = 'NTLM' AND windows-event-log:logon_type = 3 AND process:parent_image_ref.name = 'WmiPrvSE.exe']",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c4-ptt-rdp-combo",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Combined Attack: Pass-the-Ticket + RDP",
      "description": "Critical correlation detecting Kerberos RC4 downgrade with RDP access indicating advanced compromise. Rule: 110053",
      "pattern": "[windows-event-log:event_id = 4768 AND windows-event-log:ticket_encryption_type = '0x17' AND network-traffic:dst_port = 3389]",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c5-kerberoast-ptt",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Attack Chain: Kerberoasting + Pass-the-Ticket",
      "description": "High severity correlation indicating APT activity through multiple Kerberos service ticket requests. Rule: 110054",
      "pattern": "[windows-event-log:event_id = 4769 AND windows-event-log:frequency > 5]",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
    {
      "type": "indicator",
      "spec_version": "2.1",
      "id": "indicator--c6-massive-lateral",
      "created": "2025-11-19T00:00:00.000Z",
      "modified": "2025-11-19T00:00:00.000Z",
      "name": "Massive Lateral Movement",
      "description": "Critical correlation detecting sophisticated attack through massive RDP lateral movement from single source. Rule: 110055",
      "pattern": "[network-traffic:dst_port = 3389 AND windows-event-log:logon_type = 10 AND windows-event-log:frequency > 5]",
      "pattern_type": "stix",
      "valid_from": "2025-11-19T00:00:00.000Z",
      "indicator_types": ["anomalous-activity", "malicious-activity"]
    },
